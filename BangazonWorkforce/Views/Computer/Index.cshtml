            <td>
                @Html.DisplayFor(modelItem => computer.Id)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.PurchaseDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DecomissionDate)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => computer.Make)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.Manufacturer)
            </td>*@
